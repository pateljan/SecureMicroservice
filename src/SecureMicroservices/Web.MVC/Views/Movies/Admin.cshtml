@model Web.MVC.Models.UserInfoViewModel
@{
    ViewData["Title"] = "Administrator";
}

<h1>Administrator</h1>

<table>
    <thead>
        <tr>
            <th>Claim Type</th>
            <th>Claim value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.UserInfoDictionary)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Key)</td>
                <td>
                    @Html.DisplayFor(modelItem => item.Value)
                </td>
            </tr>
        }

    </tbody>
</table>
